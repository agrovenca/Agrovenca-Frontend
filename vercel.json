{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.agrovenca.com/:path*"
    },
    { "source": "/(.*)", "destination": "/" }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.agrovenca.com" }],
      "destination": "https://agrovenca.com/:1",
      "permanent": true
    }
  ]
}
